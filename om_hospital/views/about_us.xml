<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Header Extended -->
    <template id="om_hospital.saned_header_custom" inherit_id="website.layout" name="Header">
        <xpath expr="//header" position="replace">
            <head>
                <link rel="stylesheet" type="text/css" href="/om_hospital/static/src/css/home.css"/>
                <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous"/>
            </head>
            <div id="webheader-nav">
                <nav class="navbar navbar-light navbar-expand-lg ">
                    <div class="container">
                        <a class="navbar-brand" href="#">
                            <img src="/om_hospital/static/src/img/om.png" class="logosizenav"></img>
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>
                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="navbar-nav ml-auto">
                                <li class="nav-item active">
                                    <a class="nav-link" href="/">Home</a>
                                </li>
                                <li class="nav-item active">
                                    <a class="nav-link" href="/about_us">About Us</a>
                                </li>
                                <li class=" signinbtn" groups="base.group_public">
                                    <a class="" href="/web/login">
                                        <span>Sign In</span>
                                    </a>
                                </li>
                                <li class="nav-item divider" t-ignore="true" t-if="not user_id._is_public()" />
                                <li class="nav-item dropdown" t-ignore="true" t-if="not user_id._is_public()">
                                    <a href="#" class="nav-link dropdown-toggle" data-toggle="dropdown">
                                        <b>
                                            <span t-esc="user_id.sudo().name[:23] + '...' if user_id.sudo().name and len(user_id.sudo().name) &gt; 25 else user_id.sudo().name" />
                                        </b>
                                    </a>
                                    <div class="dropdown-menu js_usermenu" role="menu">
                                        <a href="/" role="menuitem" class="dropdown-item">My Account</a>
                                        <a id="o_logout" class="dropdown-item" t-attf-href="/web/session/logout?redirect=/" role="menuitem">Logout</a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
            </div>
        </xpath>
