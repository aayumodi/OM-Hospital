    <!-- sign in and signup page extended -->
    <template id="saned_website_signin_page" name="saned_signin_page" inherit_id="web.login" active="True">
        <xpath expr="//t/form" position="replace">
            <div class="container-fluid">
                <div class="row pt-5 pb-5">
                    <div class="shadow-box signinup-form-box " id="signinsignupfrm">
                        <div class="row">
                            <div class="col-md-6 col-12 blue-btm-ptrn blue-main">
                                <div class="blue-circle"></div>
                                <div class="img-left-form">
                                    <img src="/acs_website/static/src/img/doctorsignin.png"></img>
                                </div>
                            </div>
                            <div class="col-md-6 col-12 pd-lft-zero ">
                                <div class="white-bg-form">
                                    <div class="form-wrap ">
                                        <div class="tabs-form">
                                            <h3 class="login-tab">
                                                <a class="active" href="#login-tab-content">Sign In</a>
                                            </h3>
                                            <h3 class="signup-tab">
                                                <a href="#signup-tab-content">Sign Up</a>
                                            </h3>
                                        </div>
                                        <!--.tabs-->
                                        <div class="tabs-content">
                                            <div id="signup-tab-content">
                                                <form class="signup-form" role="form" method="post" t-if="not message" t-attf-action="/web/signup" onsubmit="this.action = this.action + location.hash">
                                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                                                    <label class="input-label">User Name</label>
                                                    <input type="text" class="input" name="name" id="name" t-att-value="name" autocomplete="off" placeholder="" autofocus="autofocus" autocapitalize="off" required="required" t-att-readonly="'readonly' if only_passwords else None" t-att-autofocus="'autofocus' if login and not only_passwords else None" />
                                                    <label class="input-label">Email</label>
                                                    <input type="text" class="input" name="login" t-att-value="login" id="login" autocomplete="off" autofocus="autofocus" placeholder="" autocapitalize="off" required="required" t-att-readonly="'readonly' if only_passwords else None" />
                                                    <label class="input-label">Password</label>
                                                    <input type="password" name="password" class="input" id="password" required="required" autocomplete="off" placeholder="" t-att-autofocus="'autofocus' if only_passwords else None" />
                                                    <label class="input-label">Confirm Password</label>
                                                    <input type="password" name="confirm_password" class="input" id="confirm_password" autocomplete="off" placeholder="" required="required" />
                                                    <p class="alert alert-danger" t-if="error" role="alert">
                                                        <t t-esc="error" />
                                                    </p>
                                                    <input type="hidden" name="redirect" t-att-value="redirect" />
                                                    <input type="hidden" name="token" t-att-value="token" />
                                                    <input type="submit" class="button signup-btn mt-3" value="Sign Up" />
                                                </form>
                                                <!--.login-form-->
                                                <div class="help-text">
                                                    <div class="form-link-section">
                                                        <p>Don't have an account ? </p>
                                                        <a href="#" class="signin-link" id="open-signin">Sign In</a>
                                                    </div>
                                                    <!-- <p><a href="#">Terms of service</a></p> -->
                                                </div> <!--.help-text-->
                                            </div>
                                            <!--.signup-tab-content-->
                                            <div id="login-tab-content" class="active">
                                                <form class="login-form" role="form" t-attf-action="/web/login" method="post" onsubmit="this.action = this.action + location.hash">
                                                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                                                    <label class="input-label">Email</label>
                                                    <input type="text" class="input" id="login" name="login" autocomplete="off" t-att-value="login" required="required" placeholder="" autofocus="autofocus" autocapitalize="off" />
                                                    <label class="input-label">Password</label>
                                                    <input type="password" class="input" id="password" name="password" required="required" autocomplete="off" t-att-autofocus="'autofocus' if login else None" placeholder="" maxlength="4096" />
                                                    <div>
                                                        <div class="custom-control custom-checkbox pt-4 pb-4">
                                                            <input type="checkbox" class="custom-control-input" id="customCheckBox1" />
                                                            <label class="custom-control-label" for="customCheckBox1">Remember me</label>
                                                            <a href="/web/reset_password?" class="remember-me-text ml-auto reset-pass">Reset OM Password</a>
                                                        </div>
                                                    </div>
                                                    <p class="alert alert-danger" t-if="error" role="alert">
                                                        <t t-esc="error" />
                                                    </p>
                                                    <p class="alert alert-success" t-if="message" role="status">
                                                        <t t-esc="message" />
                                                    </p>
                                                    <input type="hidden" name="redirect" t-att-value="redirect" />
                                                    <input type="submit" class="button" value="Login" />
                                                </form> <!--.login-form-->
                                                <div class="help-text">
                                                    <div class="form-link-section">
                                                        <p>Already have an account ? </p>
                                                        <a href="#" class="signin-link" id="open-signup">Sign Up</a>
                                                    </div>
                                                </div> <!--.help-text-->
                                            </div> <!--.login-tab-content-->
                                        </div> <!--.tabs-content-->
                                    </div> <!--.form-wrap-->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </xpath>
    </template>
